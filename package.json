{
  "name": "nextjs-webapp-starter",
  "version": "0.2.0-canary.0",
  "description": "Up-to-date, reasonably opinionated starter template for Next.js web apps",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "release-major": "npm version major -m \"chore(release): version %s\"",
    "release-minor": "npm version minor -m \"chore(release): version %s\"",
    "release-patch": "npm version patch -m \"chore(release): version %s\"",
    "version": "git add .",
    "postversion": "git push && git push --follow-tags"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/owanhunte/nextjs-webapp-starter"
  },
  "engines": {
    "node": ">= 10.21.0"
  },
  "dependencies": {
    "next": "9.5.2",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.46",
    "husky": "^4.2.5",
    "tailwindcss": "^1.6.2",
    "typescript": "^3.9.7"
  }
}
